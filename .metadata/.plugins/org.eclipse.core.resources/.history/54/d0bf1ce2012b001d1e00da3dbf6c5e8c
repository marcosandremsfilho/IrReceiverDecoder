#include "main.h"

#ifndef SENSORIRCLASS
#define SENSORIRCLASS

class SensorIR {
protected:
	uint16_t timeON, timeOFF, time;
	uint32_t decodeSignal, Signal, timeSignal[32], timeSignalSONY[16];
